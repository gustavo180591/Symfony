{% extends 'base.html.twig' %}

{% block title %}Salida{% endblock %}

{% block body %}
    <h1>Salida</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ salida.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ salida.fecha ? salida.fecha|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ salida.cantidad }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_salida_index') }}">back to list</a>

    <a href="{{ path('app_salida_edit', {'id': salida.id}) }}">edit</a>

    {{ include('salida/_delete_form.html.twig') }}
{% endblock %}
